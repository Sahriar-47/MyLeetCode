class Solution {
public:
    int makeTheIntegerZero(int num1, int num2) {
    //     if(num1 < num2) {
    //         return -1;
    //     }

    //     if(num2 >= 0) {
    //         int num = num1-num2;
    //         int bitCnt = __builtin_popcount(num);
    //         return bitCnt;
    //     }
    //     else {
    //         int num = num1 + (-1)*num2;
    //         int bitCnt = __builtin_popcount(num);
    //         return bitCnt+1;
    //     }

        int k = 1;
        while(true) {
            long long x = num1 - (1LL*num2 * k);
            if(x < k) {
                return -1;
            }

            int bitCnt = __builtin_popcountll(x);
            if(bitCnt <= k) {
                return k;
            }

            k++;
        }
    }
};
