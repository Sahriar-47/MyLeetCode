class Solution {
    public int maxProfit(int[] prices) {
        int n = prices.length;
        
        int buyAtPrice = Integer.MAX_VALUE;
        int maxProfit = 0;

        for(int i=0; i<n; i++) {
            buyAtPrice = Math.min(buyAtPrice, prices[i]);
            maxProfit = Math.max(maxProfit, prices[i]-buyAtPrice);
        }

        return maxProfit;
    }
}
