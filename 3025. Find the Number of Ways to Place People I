class Solution {
public:
    int numberOfPairs(vector<vector<int>>& points) {
        int n = points.size();

        sort(points.begin(), points.end());
        bool ok = false;
        int ans = 0;
        for(int i=0; i<n-1; i++) {
            for(int j=i+1; j<n; j++) {
                if(i != j && points[i][0] <= points[j][0] && points[i][1] >= points[j][1] || points[i][0] >= points[j][0] && points[i][1] <= points[j][1]) {
                    for(int k=0; k<n; k++) {
                        if(k!=i && k!=j && points[k][0] <= max(points[i][0], points[j][0]) && points[k][0] >= min(points[i][0], points[j][0])  &&  points[k][1] <= max(points[i][1], points[j][1]) && points[k][1] >= min(points[i][1], points[j][1])) {
                            ok = true;
                            break;
                        }
                    }
                    if(!ok) {
                        ans++;
                    }
                    ok = false;
                }
            }
        }

        return ans;
    }
};
